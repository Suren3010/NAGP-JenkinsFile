pipeline {
    agent any
    stages {
        stage('Clone project') {
            steps {
                echo 'Cloning project...'
                checkout changelog: false, poll: false, scm: [$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Suren3010/Sample.Project.git']]]
                echo 'Project cloned...'
            }
        }
    }
}